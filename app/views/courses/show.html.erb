<div class="page-content">
  <h1 class="course"><strong>Course Name</strong></h1>
  <div>
    <div class ="well col-md-3 text-center">
      <p><%= image_tag(@course.image , alt: "#{@course.name} pic", class: 'img-thumbnail course-image') %></p>
      <p><%= @course.name %></p>
      <p><%= @course.location %></p>
      <p><%= link_to "View Tee Times", tee_times_path(@course.id) %></p>
    </div>
  </div>
</div>
